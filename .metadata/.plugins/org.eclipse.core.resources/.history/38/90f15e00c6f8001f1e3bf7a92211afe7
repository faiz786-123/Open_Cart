package Test1;

import java.util.Arrays;

public class Test4 {

	public static void main(String[] args) {
		
		String str1 = "ram";
		String str2 = "arm";
		
		char c1 [] = str1.toCharArray();
		char c2 [] = str2.toCharArray();
		Arrays.sort(c1);
		Arrays.sort(c2);
		if(Arrays.equals(c1, c2)==true)
		{
			System.out.println("They are anamgram");
		}
		
		else
		{
			System.out.println("They are not anamgram");
		}
	
}
}