package Test1;

import java.util.Arrays;

public class Test4 {

	public static void main(String[] args) {
		
		String input = "DAD";
		String output = "";
		for(int i = input.length()-1; i>=0; i--)
			output = output + input.charAt(i);
		boolean b1 = input.equals(output);
		System.out.println(b1);
		
}
}